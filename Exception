import java.util.Scanner; 
class Main{ 
    public static void main(String[] args){   
        Scanner sc=new Scanner(System.in);
        int N; 
        N=sc.nextInt();
        int [] arr=new int[N]; 
        for(int i=0;i<N;i++){ 
            arr[i]=sc.nextInt(); 
        } 
        System.out.println(arr[N]+arr[N+1]); 
        
    } 
} 
